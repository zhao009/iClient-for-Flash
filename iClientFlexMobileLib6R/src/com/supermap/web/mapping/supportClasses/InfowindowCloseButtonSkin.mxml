    <s:BitmapImage source="@Embed(source='/../assets/images/component/infowindow/closeDown.png')" includeIn="down,over"/>
	<s:BitmapImage source="@Embed(source='/../assets/images/component/infowindow/closeUp.png')" includeIn="up"/>

